{
  "name": "genesys-cloud-data-extractor",
  "version": "1.0.0",
  "description": "TypeScript project for extracting data from Genesys Cloud using PureCloud Platform Client v2",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node --watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run lint && npm run type-check"
  },
  "keywords": [
    "genesys",
    "cloud",
    "typescript",
    "data-extraction",
    "purecloud"
  ],
  "author": "Johan van Zijl",
  "dependencies": {
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "luxon": "^3.5.0",
    "purecloud-platform-client-v2": "^230.0.0",
    "tslib": "^2.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^4.16.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "GPL-3.0-only"
}
